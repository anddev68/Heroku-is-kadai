<%= stylesheet_link_tag "inside/game", :media => "all" %>
<%= stylesheet_link_tag "inside/tinder", :media => "all" %>



<div class="wrapper">
  
  <h1>男の娘 鑑定人</h1><br>

  <div id="tinder-container">

    <% @subjects.each do |girl| %>
      <div class="buddy" style="display: block;" data-sex="<%= girl[1] %>">
        <div class="avatar" 
          style="display: block; background-image: url(<%= girl[0].gsub('public', '') %>)">
        </div>  
      </div>
    <% end %>
    

  </div>



</div>
